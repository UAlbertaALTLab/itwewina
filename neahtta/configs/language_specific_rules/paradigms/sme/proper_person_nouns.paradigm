name: "Proper person nouns"
description: "Generates a paradigm for proper nouns."
morphology:
  pos: N
lexicon:
  XPATH:
    sem_type: ".//l/@sem_type"
    type: ".//l/@type"
  type: Prop
  sem_type: 
    - "mal"
    - "Mal"
    - "fem"
    - "Fem"
    - "Ani"
    - "ani"
    - "Org"
    - "org"

--
{{ lemma }}+N+Prop+Sem/Mal+Sg+Nom
{{ lemma }}+N+Prop+Sem/Mal+Sg+Acc
{{ lemma }}+N+Prop+Sem/Mal+Sg+Gen
{{ lemma }}+N+Prop+Sem/Mal+Sg+Loc
{{ lemma }}+N+Prop+Sem/Mal+Sg+Ill
{{ lemma }}+N+Prop+Sem/Mal+Sg+Com

{{ lemma }}+N+Prop+Sem/Fem+Sg+Nom
{{ lemma }}+N+Prop+Sem/Fem+Sg+Acc
{{ lemma }}+N+Prop+Sem/Fem+Sg+Gen
{{ lemma }}+N+Prop+Sem/Fem+Sg+Loc
{{ lemma }}+N+Prop+Sem/Fem+Sg+Ill
{{ lemma }}+N+Prop+Sem/Fem+Sg+Com

{{ lemma }}+N+Prop+Sem/Ani+Sg+Nom
{{ lemma }}+N+Prop+Sem/Ani+Sg+Acc
{{ lemma }}+N+Prop+Sem/Ani+Sg+Gen
{{ lemma }}+N+Prop+Sem/Ani+Sg+Loc
{{ lemma }}+N+Prop+Sem/Ani+Sg+Ill
{{ lemma }}+N+Prop+Sem/Ani+Sg+Com

{{ lemma }}+N+Prop+Sem/Org+Sg+Nom
