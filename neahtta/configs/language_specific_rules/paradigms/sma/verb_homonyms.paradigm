name: "verb homonymy (plus valence)"
morphology:
  pos: V
lexicon:
  XPATH:
    homonymy: ".//l/@hid"
    valence: ".//l/@type"
  homonymy:
    - "Hom1"
    - "Hom2"
--
{{ lemma }}+{{ homonymy }}+V+Ind+Prs+Sg1
{{ lemma }}+{{ homonymy }}+V+Ind+Prs+Sg3
{{ lemma }}+{{ homonymy }}+V+Ind+Prs+Pl3
{{ lemma }}+{{ homonymy }}+V+Ind+Prt+Sg1
{{ lemma }}+{{ homonymy }}+V+ConNeg
{{ lemma }}+{{ homonymy }}+V+PrfPrc
{{ lemma }}+{{ homonymy }}+V+Ger
{{ lemma }}+{{ homonymy }}+V+VGen
