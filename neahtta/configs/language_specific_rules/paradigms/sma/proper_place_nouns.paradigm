name: "proper nouns with semantic type"
morphology:
  pos: N
  type: Prop
lexicon:
  XPATH:
    sem_type: ".//l/@sem_type"
  sem_type: 
    - "Plc"
    - "Org"
    - "Obj"
--
{{ lemma }}+N+Prop+Sem/{{ sem_type }}+Sg+Gen
{{ lemma }}+N+Prop+Sem/{{ sem_type }}+Sg+Ill
{{ lemma }}+N+Prop+Sem/{{ sem_type }}+Sg+Ine
{{ lemma }}+N+Prop+Sem/{{ sem_type }}+Sg+Ela
