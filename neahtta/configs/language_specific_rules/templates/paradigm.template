<dl class="dl-horizontal miniparadigm">
{%- for form in paradigm -%}

    {% if form.tag %}
        <dt>{{ form.tag|tagfilter(_from, _to)  }}</dt>
    {% endif %}

    {% if form.form %}
    <dd>{{ form.form }}</dd>
    {% else %}
        <dd>?</dd>
    {% endif %}
{%- endfor -%}
</dl>

